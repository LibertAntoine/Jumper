{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jumper",
  "version": "0.1.0",
  "identifier": "com.jumper.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "decorations": false,
        "title": "Jumper",
        "width": 1200,
        "height": 680,
        "minHeight": 380,
        "minWidth": 330
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "externalBin": ["bin/python/test"],
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
